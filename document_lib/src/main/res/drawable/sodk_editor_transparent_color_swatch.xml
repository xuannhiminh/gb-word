<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="@dimen/_20sdp"
                android:height="@dimen/_20sdp" />
            <stroke
                android:width="1dp"
                android:color="@color/sodk_editor_palette_black" />
            <solid android:color="@color/sodk_editor_palette_white" />
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="45.0">
            <shape android:shape="line">
                <stroke
                    android:width="4.0dip"
                    android:color="@color/sodk_editor_palette_red" />
            </shape>
        </rotate>
    </item>
</layer-list>